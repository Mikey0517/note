  "main": "index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --config ./config/nodemon.json",
    "server:dev": "cross-env NODE_ENV=production nodemon server/index.js --config ./config/nodemon.json",
    "server": "cross-env NODE_ENV=production node server/index.js",
    "build": "npm run build:prod && npm run build:ssr",
    "build:prod": "cross-env NODE_ENV=production ENABLE_SSR=true webpack --progress -p --config ./config/webpack.config.prod.js",
    "build:ssr": "cross-env NODE_ENV=production ENABLE_SSR=true webpack --progress -p --config ./config/webpack.config.ssr.js",
    "build:analyzer": "cross-env NODE_ENV=production BUNDLE_ANALYZER=true webpack --progress -p --config ./config/webpack.config.prod.js",
    "start": "npm run build && npm run server"
  },
